pragma solidity 0.7.3;

import { ILendingPool } from "../interfaces/ILendingPool.sol";
import "../interfaces/ILendingPoolAddressesProvider.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "hardhat/console.sol";
import "../interfaces/IUniswapV2Router02.sol";
import "../interfaces/IAaveIncentivesController.sol";
import "../interfaces/IVault.sol";
import "./DividendToken.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Pausable.sol";
import "hardhat/console.sol";
import "../interfaces/IDistribution.sol";

contract Aave is ERC20, ERC20Detailed, IUpgradeSource, ControllableInit, VaultStorage, Ownable, Pausable, DividendToken {
    using SafeMath for uint256;
    using SafeERC20 for IERC20Detailed;
    IERC20Detailed public underlying;
    IStrat public strat;
    Timelock public timelock;
    address private lendingPoolAddressProviderAddress =
      0xd05e3E715d945B59290df0ae8eF85c1BdB684744;
    address[] public aDai = [0x27F8D03b3a2196956ED754baDc28D73be8830A6e];
    IUniswapV2Router02 quickSwap =
      IUniswapV2Router02(0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff);
    IAaveIncentivesController rewards =
      IAaveIncentivesController(0x357D51124f59836DeD84c8a1730D72B749d8BC23);
    uint256 public depositId;
    address public matic = 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270;
    address public dai = 0x8f3Cf7ad23Cd3CaDbD9735AFf958023239c6A063;
    // address private usdtAddress = 0xdAC17F958D2ee523a2206206994597C13D831ec7;
    // address private aUsdtAddress = 0x71fc860F7D3A592A4a98740e39dB31d25db65ae8;
    event Withdraw(address indexed beneficiary, uint256 amount);
    event Deposit(address indexed beneficiary, uint256 amount);
    event Invest(uint256 amount);
    event StrategyAnnounced(address newStrategy, uint256 time);
    event StrategyChanged(address newStrategy, address oldStrategy);


  constructor(IERC20Detailed underlying_, IERC20 reward_, address harvester_, string memory name_, string memory symbol_)
    DividendToken(reward_, name_, symbol_, underlying_.decimals())
    {
        underlying = underlying_;
        harvester = harvester_;
        depositLimit = 20000 * (10**underlying_.decimals()); // 20k initial deposit limit
        timelock = new Timelock(msg.sender, 2 days);
        _pause(); // paused until a strategy is connected
    }
    modifier onlyHarvester {
        require(msg.sender == harvester);
        _;
    }
    function getLendingPool() public view returns (address) {
      return
        ILendingPoolAddressesProvider(lendingPoolAddressProviderAddress)
          .getLendingPool();
    }
    function depositFunds(
      uint256 transferAmount
    ) public returns (bool success) {
      IERC20 _token = IERC20(dai);
      require(_token.allowance(msg.sender, address(this)) >= transferAmount);
      _token.safeTransferFrom(msg.sender, address(strat), amount);
      _token.safeTransferFrom(msg.sender, address(this), transferAmount);
      _token.safeApprove(getLendingPool(), transferAmount);
      ILendingPool(getLendingPool()).deposit(
        tokenAddress,
        transferAmount,
        address(this),
        0
      );
      uint256 toMint = totalSupply() == 0
          ? amount
          : amount.mul(totalSupply()).div(underlyingBalanceWithInvestment());
      _mint(beneficiary, toMint);

      IERC20(underlying()).safeTransferFrom(sender, address(this), amount);

      // update the contribution amount for the beneficiary
      emit Deposit(beneficiary, amount);
      return true;
    }

    function withdraw(uint amount) external {
        IERC20 _token = IERC20(dai);
        require(amount > 0, "ZERO-AMOUNT");
        burn(msg.sender, amount);
        ILendingPool(getLendingPool()).withdraw(
          dai,
          amount,
          address(this)
        );
        _token.safeTransfer(msg.sender, amount);
    }

    function harvest(uint amount) external onlyHarvester returns (uint afterFee) {
        require(amount <= underlyingYield(), "Amount larger than generated yield");
        ILendingPool(getLendingPool()).claimRewards();
        address[] memory path = new address[](2);
        path[0] = matic;
        path[1] = dai;

        from.approve(address(ROUTER), afterFee);
        uint256 received = ROUTER.swapExactTokensForTokens(
          afterFee,
          1,
          path,
          address(this),
          block.timestamp + 1
        )[path.length - 1];

        to.approve(address(vault), received);
        vault.distribute(received);

        emit Harvested(address(vault), msg.sender);
      
    } 
    function distribute(uint amount) internal onlyHarvester {
      distributeDividends(amount);
      lastDistribution = block.timestamp;
    }
}                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               